<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

  <module name="amc" package="com.axelor.amc.db" />

  <entity name="AmcEntry">
    <string name="reference" readonly="true" title="Reference"/>
    <string name="name" title="Amc Name" />
    <string name="typeSelect" selection="amcEntry.type.selection" title="Amc Type" />
    <decimal name="price" title="Amc Price"/>
    <integer name="nOfService" title="Total Service"/>
    <integer name="nOfYear" title="Total Year"/>
    <string name="remark" title="Service Remarks"/>
    <boolean name="isActive" default="true" title="Is Active"/>
  </entity>
</domain-models>
